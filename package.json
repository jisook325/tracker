{
  "name": "tracker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "pages:build": "next build && next-on-pages",
    "start": "next start",
    "lint": "eslint",
    "worker:dev": "wrangler dev worker/src/index.ts --local",
    "worker:deploy": "wrangler deploy worker/src/index.ts",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrate:local": "wrangler d1 migrations apply DB --local"
  },
  "dependencies": {
    "drizzle-orm": "^0.35.3",
    "next": "15.5.2",
    "next-auth": "^4.24.13",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.16",
    "@cloudflare/workers-types": "^4.20241230.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "drizzle-kit": "^0.28.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.54.0"
  }
}
